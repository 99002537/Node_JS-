    <script>
        $(document).ready(function(){
            $('#mybutton').click(function(){
                $.get("hello.txt",function(data,status){
                     $('#mydiv').html('<b>'+ data+ '</b>');
                });
            });
        });
    </script>
</head>
<body>
    <button id="mybutton">Show message</button>
